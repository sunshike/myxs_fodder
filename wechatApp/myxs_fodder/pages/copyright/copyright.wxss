/* myxs_fodder/pages/copyright/copyright.wxss */

page {
  font-family: 微软雅黑;
}

.copyright {
  background-color: #f9f9f9;
  padding-top: 5px;
  position: relative;
}

.logo {
  width: 100%;
  height: 250px;
}

.copyright>view {
  background-color: rgb(255, 255, 255);
  box-shadow: 0px 1px 1px #a8a6a6;
  margin: auto;
  min-height: 60px;
  border-top: #e6e6e6 solid 1px;
  border-radius: 5px;
}

.copyright>view:nth-child(1) {
  width: 90%;
  margin-top: -85px;
}

.copyright>view:nth-of-type(n+2) {
  width: 90%;
  margin-top: 35px;
}

.h65 {
  height: 65px;
}

.tip {
  position: absolute;
  top: 100px;
  font-size: 18px;
  left: 0;
  right: 0;
  text-align: center;
  color: #fff;
  font-weight: 100;
}

.tatle {
  border-bottom: #ddd dashed 1px;
  padding-top: 5px;
  padding-bottom: 5px;
  font-size: 16px;
  height: 45px;
  line-height: 45px;
}

.info {
  font-size: 14px;
  color: #000;
  padding-top: 10px;
  padding-bottom: 10px;
}

.shouqi {
  overflow: hidden;
  height: 60px;
}

.show {
}

.text {
  color: rgb(54, 54, 54);
  font-weight: 100;
}

.ShowText {
  text-align: center;
  font-size: 12px;
  color: #949496;
  padding-top: 5px;
  padding-bottom: 5px;
  border-top: #ddd dashed 1px;
}

.DateStyle {
  font-size: 14px;
  color: rgb(192, 192, 192);
  font-weight: 100;
}

.DateStyles {
  color: rgb(192, 192, 192);
}

.lxkfz {
  text-align: center;
  color: #ddd;
  font-size: 12px;
  line-height: 115px;
}

.indexS {
  position: fixed;
  left: 17px;
  top: 0;
  color: #fff;
  height: 35px;
  padding-top: 30px;
  z-index: 1000;
  line-height: 25px;
}
